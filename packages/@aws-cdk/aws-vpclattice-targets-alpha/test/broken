test('Bad HealthyThresholdCount', () => {
      expect(() => {
        new ApplicationLoadBalancer(stack, 'ALBTarget1', {
          alb: [loadbalancer],
          targetConfig: {
            vpc: vpc1,
            healthcheck: HealthCheck.check({
              healthyThresholdCount: 0,
            }),
          },
        });
      }).toThrowError('HealthyThresholdCount must be between 1 and 10');
    });

    test('Bad unhealthyThresholdCount', () => {
      expect(() => {
        new ApplicationLoadBalancer(stack, 'ALBTarget1', {
          alb: [loadbalancer],
          targetConfig: {
            vpc: vpc1,
            healthcheck: HealthCheck.check({
              unhealthyThresholdCount: 0,
            }),
          },
        });
      }).toThrowError('UnhealthyThresholdCount must be between 2 and 10');
    });